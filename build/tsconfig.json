{
  "compileOnSave": true,
  "compilerOptions": {
    "allowJs": true,
    "allowUnreachableCode": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": false,
    "noEmitOnError": false,
    "moduleResolution":"node",
    "target": "es5",
    "lib": [ "es2015","dom"],
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "types":["node"],
    "noImplicitUseStrict": true
  },
  "exclude": [
    "node_modules"
  ]
}
